<h1><%=@robot.name%></h1>
  <img src="http://www.robohash.org/<%= @robot.name %>">
  <p><%= @robot.city %></p>
  <p><%= @robot.state %></p>
  <p><%= @robot.department %></p>

<button><a href="/robots/<%= @robot.id %>/edit">Edit this Robot</a></button>
<form action="/robots/<%= @robot.id %>" method="POST">
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete this Robot"/>
</form>
<h5><a href="/robots">Back to Robots List</a></h5>
